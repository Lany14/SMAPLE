model ClinicVisitBookingHistory {
  id                String            @id @default(auto()) @map("_id") @db.ObjectId
  vetDoctor DoctorProfile @relation(name: "DoctorClinicVisitHistories", fields: [doctorId, doctorFirstName, doctorLastName, doctorEmail ], references: [doctorId, doctorFirstName, doctorLastName, doctorEmail], onDelete: Cascade, onUpdate: Cascade)
  doctorId         String      @db.ObjectId
  doctorFirstName String
  doctorLastName String
  doctorEmail     String

  pet               Pet               @relation(name: "PetsClinicVisitHistories", fields: [petId, petName], references: [petId, petName], onDelete: Cascade, onUpdate: Cascade)
  petId             Int               @unique
  petName           String

  petOwner          PetOwnerProfile   @relation(name: "PetOwnerClinicVisitHistories", fields: [petOwnerId, petOwnerFirstName, petOwnerLastName, petOwnerEmail], references: [petOwnerId, petOwnerFirstName, petOwnerLastName, petOwnerEmail], onDelete: Cascade, onUpdate: Cascade)
  petOwnerId        String            @db.ObjectId
  petOwnerFirstName String
  petOwnerLastName  String
  petOwnerEmail     String

  bookingId         String            @unique 
  message           String
}