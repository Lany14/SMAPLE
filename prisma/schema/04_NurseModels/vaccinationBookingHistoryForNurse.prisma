model VaccinationBookingHistoryForNurse {
  id                String            @id @default(auto()) @map("_id") @db.ObjectId

  nurse                        NurseProfile     @relation(name: "NurseVaccinationBookingHistories", fields: [nurseId, nurseFirstName, nurseLastName, nurseEmail], references: [nurseId, nurseFirstName, nurseLastName, nurseEmail], onDelete: Cascade, onUpdate: Cascade)
  nurseId                       String            @db.ObjectId
  nurseFirstName               String
  nurseLastName      String
  nurseEmail String

  pet               Pet               @relation(name: "PetsVaccinationBookingHistories", fields: [petId, petName], references: [petId, petName], onDelete: Cascade, onUpdate: Cascade)
  petId             Int               @unique
  petName           String

  petOwner          PetOwnerProfile   @relation(name: "PetOwnerVaccinationBookingHistories", fields: [petOwnerId, petOwnerFirstName, petOwnerLastName, petOwnerEmail], references: [petOwnerId, petOwnerFirstName, petOwnerLastName, petOwnerEmail], onDelete: Cascade, onUpdate: Cascade)
  petOwnerId        String            @db.ObjectId
  petOwnerFirstName String
  petOwnerLastName  String
  petOwnerEmail     String

  bookingId         String            @unique 
  message           String
}